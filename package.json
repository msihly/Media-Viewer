  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder build --win -c.extraMetadata.main=build/main.js --publish never",
    "postinstall": "electron-builder install-app-deps && patch-package",
    "dev": "concurrently --kill-others \"SET BROWSER=none && npm start\" \"electron .\""
  },
  "build": {
    "appId": "com.msihly.media-viewer",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "artifactName": "Media-Viewer.exe",
      "icon": "build/app-icon.ico",
      "target": "portable"
    }
  },
  "proxy": "http://localhost:3000",
  "browserslist": {
    "production": [
      "last 3 chrome version"
    ],
    "development": [
      "last 3 chrome version"
    ]
  }
}
