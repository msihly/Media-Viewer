{
  "name": "media-viewer",
  "version": "0.1.0",
  "private": true,
  "author": "Mohamed Sihly",
  "description": "A tag-based media-viewer",
  "homepage": "./",
  "main": "public/electron.js",
  "icon": "public/favicon.ico",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@electron/remote": "^2.0.8",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/core": "^5.0.0-alpha.54",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "^5.5.2",
    "@mui/styles": "^5.5.1",
    "color": "^4.2.3",
    "dayjs": "^1.11.0",
    "directory-tree": "^2.2.9",
    "fluent-ffmpeg": "^2.1.2",
    "jsonfile": "^6.1.0",
    "md5-file": "^5.0.0",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "mobx-state-tree": "^5.1.3",
    "mongoose": "^6.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-rnd": "^10.3.4",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-selecto": "^1.15.0",
    "react-toastify": "^8.2.0",
    "sass": "^1.49.9",
    "sharp": "^0.30.3",
    "tss-react": "^3.6.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/sharp": "^0.29.5",
    "babel-loader": "8.1.0",
    "concurrently": "^7.2.2",
    "electron": "^20.0.3",
    "electron-builder": "^22.14.13",
    "electron-devtools-installer": "^3.2.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "react-error-overlay": "6.0.9",
    "reactotron-mst": "^3.1.4",
    "reactotron-react-js": "^3.3.7",
    "sass-loader": "^10.1.1",
    "ts-loader": "^8.2.0",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.1",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "cracoConfig": "craco.config.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "craco start",
    "build": "craco build",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently \"SET BROWSER=none && npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "build": {
    "appId": "com.msihly.media-viewer",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "artifactName": "Media-Viewer.exe",
      "icon": "build/app-icon.ico",
      "target": "portable"
    }
  },
  "proxy": "http://localhost:3000",
  "browserslist": {
    "production": [
      "last 3 chrome version"
    ],
    "development": [
      "last 3 chrome version"
    ]
  }
}
